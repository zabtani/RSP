{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest --run",
    "lint": "eslint --cache --fix src/*/**.{tsx,ts}",
    "format": "prettier --write src/*.{tsx,ts}",
    "isready": "npm run lint && npm run format && npm run build && npm run test:run",
    "generate-icons": "npx @svgr/cli --config-file=.svgrrc.json ./src/assets/icons --out-dir=./src/icons"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-angular": "17.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "@vitejs/plugin-react": "2.2.0",
    "babel-loader": "9.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.27.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "20.0.2",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vite": "3.2.4",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-fonts": "0.6.0",
    "vite-plugin-svgr": "2.2.2",
    "vite-tsconfig-paths": "3.5.2"
  },
  "lint-staged": {
    "*.{js,tsx,ts,css,md}": "prettier --write",
    "*.{js,tsx,ts}": "eslint --cache --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
